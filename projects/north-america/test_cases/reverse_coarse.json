{
  "name": "reverse coarse",
  "priorityThresh": 1,
  "endpoint": "reverse",
  "tests": [
    {
      "id": 1,
      "status": "pass",
      "user": "trescube",
      "description": "layers=coarse should search all coarse layers (down to neighbourhood)",
      "type": "dev",
      "in": {
        "point.lat": 40.744260,
        "point.lon": -73.990400,
        "layers": "coarse"
      },
      "expected": {
        "properties": [
          {
            "layer": "neighbourhood",
            "neighbourhood": "Flatiron District",
            "borough": "Manhattan",
            "locality": "New York",
            "county": "New York County",
            "region": "New York",
            "country": "United States"
          }
        ]
      }
    },
